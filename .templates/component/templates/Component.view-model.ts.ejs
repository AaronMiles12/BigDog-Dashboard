import { useCallback, useState } from 'react';
import { <%=componentPropsName%> } from './<%=componentFileName%>.types';

function <%=componentUseViewModelHookName%>({ }: <%=componentPropsName%>) {
  const [counter, setCounter] = useState(0);
  const handleClick = useCallback(() => {
    setCounter((c) => c + 1);
  }, []);

  return {
    handleClick,
    counter,
  };
}

export default <%=componentUseViewModelHookName%>;
